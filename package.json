{
	"name": "es7-javascript-class-snippets",
	"displayName": "ES7 JavaScript/Node/Mongoose/MongoDB-Mysql snippets",
	"description": "Simple extension for Node, javascript, Mocha, Mysql, Mongodb, Mocha and mongoose",
	"publisher": "abrahamwilliam007",
	"keywords": [
		"snippets",
		"javascript",
		"typescript"
	],
	"version": "0.1.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/abrahamwilliam/ES7-JavaScript-Node-Mongoose-MongoDB-Mysql-snippets"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.snippetsearch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.snippetsearch",
				"title": "ES7 snippet search"
			}
		],
		"keybindings": [
			{
				"command": "extension.snippetsearch",
				"key": "ctrl+alt+r",
				"mac": "shift+cmd+r",
				"when": "editorTextFocus"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./src/snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
